{
   "dependencies": {
      "@types/node": "^10.12.12",
      "@types/uuid": "^3.4.4",
      "dotenv": "^6.2.0",
      "expo-server-sdk": "^3.1.0",
      "moment": "^2.23.0",
      "node-zookeeper-client": "^0.2.2",
      "pg": "^7.3.0",
      "queue": "git+ssh://git@github.com:budziam/menu-queue-node.git#master",
      "raven": "^2.6.4",
      "reflect-metadata": "^0.1.10",
      "string-argv": "^0.1.1",
      "typeorm": "0.2.9",
      "uuid": "^3.3.2",
      "winston": "^3.1.0",
      "winston-graylog2": "^1.0.3"
   },
   "devDependencies": {
      "@types/chai": "^4.1.4",
      "@types/chai-as-promised": "^7.1.0",
      "@types/mocha": "^5.2.5",
      "@types/sinon": "^5.0.2",
      "@types/sinon-chai": "^3.2.0",
      "chai": "^4.1.2",
      "chai-as-promised": "^7.1.1",
      "mocha": "^5.1.1",
      "mocha-junit-reporter": "^1.18.0",
      "nyc": "^13.0.1",
      "sinon": "^5.0.7",
      "sinon-chai": "^3.2.0",
      "source-map-support": "^0.5.9",
      "tslint": "^5.11.0"
   },
   "scripts": {
      "start": "ts-node src/app.ts",
      "build": "tsc",
      "test": "nyc mocha",
      "test-watch": "mocha --watch --watch-extensions ts",
      "lint": "tslint -p tsconfig.json -t stylish",
      "lint-fix": "tslint -p tsconfig.json -t stylish --fix"
   },
   "nyc": {
      "check-coverage": true,
      "statements": 50,
      "branches": 50,
      "functions": 40,
      "lines": 50,
      "reporter": [
         "text-summary",
         "lcov"
      ],
      "all": true,
      "extension": [
         ".ts"
      ],
      "include": [
         "src/**/*.ts"
      ],
      "exclude": [
         "**/*.d.ts"
      ],
      "require": [
         "ts-node/register"
      ],
      "sourcemap": true,
      "instrument": true
   }
}
